<tr class="service_items fields" id="<%= dom_id(f.object) %>" data-hook="service_item">
  <td class="move-handle text-center">
    <span class="glyphicon glyphicon-move handle"></span>
    <%= f.hidden_field :id %>
  </td>
  <td class='property_name' style="width: 25%">
    <%= f.text_field :name, class: 'autocomplete form-control' %>
  </td>
  <td class='value'>
    <%= f.text_field :price, class: 'form-control' %>
  </td>
  <td class='value'>
    <%= f.text_field :core_name, class: 'form-control' %>
  </td>
  <td class='value'>
    <%= f.text_field :core_price, class: 'form-control' %>
  </td>
  <td class='value'>
    <%= f.text_field :alternate_price, class: 'form-control' %>
  </td>
  <td class='value'>
    <%= f.text_field :service_application_id, class: 'form-control' %>
  </td>

  <td class="actions actions-1">
    <% if f.object %>
      <td class="actions actions-1 text-center"><%= link_to_icon_remove_fields f %></td>
    <% end %>
  </td>
</tr>
